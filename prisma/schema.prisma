datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


generator client {
  provider = "prisma-client-js"
}

// Stickerpack state
// user_id: store each user  who make a kang for future request
// current: store current used sticker pack.

model stickerpack_state {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  user_id Int   @unique
  current    String?
}